local wireless = "back"
local wired = "bottom"
local wiredProt = "wired"

while true do
	
	rednet.open(wireless)
	local id, msg, dist, protocol = rednet.receive()
	rednet.close(wireless)
	
	if type(msg) == "string" then
		if msg == "wired" then
			rednet.open(wireless)
			rednet.send(id, os.getComputerID(), protocol)
			rednet.close(wireless)
		end
	else
		print(msg.sProt)
		print(protocol)
	
		rednet.open(wired)
		rednet.send(rednet.lookup(msg.sProt, msg.sProt), msg, protocol)
		rednet.close(wired)	
	end

end